{
  "name": "babel-plugin-ts-starter",
  "version": "1.0.0",
  "description": "starter for creating a babel plugin with typescript",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "watch": "jest --watch",
    "lint": "eslint . && markdownlint **/*.md >/dev/null",
    "coverage": "jest --coverage src",
    "type-check": "tsc --skipLibCheck --noEmit",
    "build": "rimraf lib && yarn build:types && yarn build:prod",
    "build:types": "tsc --skipLibCheck --emitDeclarationOnly",
    "build:prod": "NODE_ENV=production babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps --copy-files"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/traverse": "^7.12.12",
    "@babel/types": "^7.12.12",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-markdown": "^2.0.0-rc.1",
    "eslint-plugin-prettier": "^3.1.3",
    "markdownlint-cli": "^0.23.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "jest": "^26.6.3"
  }
}
